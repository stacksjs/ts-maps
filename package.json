{
  "name": "ts-maps",
  "type": "module",
  "version": "0.2.3",
  "private": true,
  "description": "A modern vector map library.",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/ts-maps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/ts-maps.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/ts-maps/issues"
  },
  "keywords": [
    "maps",
    "vector",
    "map",
    "development",
    "typescript",
    "stacks",
    "typescript",
    "javascript"
  ],
  "exports": {
    ".": {
      "import": "./packages/ts-maps/dist/index.js"
    }
  },
  "module": "./packages/ts-maps/dist/index.js",
  "types": "./packages/ts-maps/dist/index.d.ts",
  "bin": {
    "post": "./packages/post/dist/bin/cli.js"
  },
  "files": [
    "README.md",
    "packages/post/dist"
  ],
  "scripts": {
    "build": "cd ./packages/ts-maps && bun run build && cd ../vue && bun run build && cd ../nuxt && bun run build && cd ../react && bun run build",
    "fresh": "bunx rimraf node_modules/ bun.lock && bun i",
    "test": "bun test",
    "lint": "bunx --bun eslint .",
    "lint:fix": "bunx --bun eslint . --fix",
    "changelog": "bunx logsmith --verbose",
    "changelog:generate": "bunx logsmith --output CHANGELOG.md",
    "release": "bun run changelog:generate && bunx --bun bumpx prompt --recursive",
    "typecheck": "bunx tsc --noEmit",
    "dev:docs": "bun --bun vitepress dev docs",
    "build:docs": "bun --bun vitepress build docs",
    "preview:docs": "bun --bun vitepress preview docs"
  },
  "devDependencies": {
    "@stacksjs/bumpx": "^0.1.86",
    "@stacksjs/clarity": "^0.3.24",
    "@stacksjs/docs": "^0.70.23",
    "@stacksjs/eslint-config": "^4.14.0-beta.3",
    "@stacksjs/gitlint": "^0.1.5",
    "@stacksjs/logsmith": "^0.1.18",
    "@types/bun": "^1.3.0",
    "@types/clean-css": "^4.2.11",
    "buddy-bot": "^0.9.8",
    "bun-git-hooks": "^0.2.19",
    "bun-plugin-dtsx": "0.21.12",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "unconfig": "0.3.10"
  },
  "git-hooks": {
    "pre-commit": {
      "staged-lint": {
        "*.{js,ts,json,yaml,yml,md}": "bunx --bun eslint --fix"
      }
    },
    "commit-msg": "bunx gitlint --edit .git/COMMIT_EDITMSG"
  },
  "workspaces": [
    "packages/**",
    "playground"
  ]
}
