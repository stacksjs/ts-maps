  },
  "dependencies": {
    "ts-maps": "workspace:*",
    "ts-maps-vue": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/typescript-build": "^3.0.2",
    "@stacksjs/eslint-config": "^4.2.1-beta.1",
    "@types/node": "^24.3.0",
    "nuxt": "^3.0.0",
